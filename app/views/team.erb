
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Team <%= @team.name %></h1>
    <h2>Team Motto: <%= @team.motto %></h2>
    <% @superheroes.each do |superhero| %>
      <h2>Hero Name: <%= superhero.name %></h2>
      <p>Hero Power: <%= superhero.power %></p>
      <p>Hero Biography: <%= superhero.bio %></p>
      <% end %>
  </body>
</html>
